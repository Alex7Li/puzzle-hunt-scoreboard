{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "worker": true
  },
  "parserOptions": {
    "ecmaVersion": 10,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "class-methods-use-this": "warn"
    "node/no-missing-import": [
  "error",
    {
      allowModules: ["firebase-admin"],
    },
  ],
  }
}